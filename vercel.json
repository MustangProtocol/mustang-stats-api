{
  "version": 2,
  "installCommand": "bun install",
  "buildCommand": "bun build src/index.ts --target=node --outdir=dist",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}